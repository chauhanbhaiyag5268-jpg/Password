{
  "app_name": "Pixellab",
  "version": "1.0.0",
  "settings": {
    "password_length": 12,
    "include_uppercase": true,
    "include_lowercase": true,
    "include_numbers": true,
    "include_symbols": true,
    "allowed_symbols": "!@#$%^&*()_+-=[]{}|;:,.<>?",
    "exclude_similar_characters": true,
    "max_history": 10
  },
  "metadata": {
    "author": "tgr_ashish",
    "license": "MIT",
    "description": "Configuration file for password generator APK integration."
  }
}
